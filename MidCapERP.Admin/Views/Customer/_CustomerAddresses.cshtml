@model IEnumerable<MidCapERP.Dto.CustomerAddresses.CustomerAddressesResponseDto>
@{
    Layout = "_Layout";
}
<table class="table c-table" id="tblCustomerAddresses">
        <thead>
            <tr>
                <th>@Html.DisplayNameFor(model => model.AddressTypeId)</th>
                <th>@Html.DisplayNameFor(model => model.Street1)</th>
                <th>@Html.DisplayNameFor(model => model.Street2)</th>
                <th>@Html.DisplayNameFor(model => model.Landmark)</th>
                <th>@Html.DisplayNameFor(model => model.City)</th>
                <th>@Html.DisplayNameFor(model => model.State)</th>
                <th>@Html.DisplayNameFor(model => model.ZipCode)</th>
                <th>@Html.DisplayNameFor(model => model.IsDefault)</th>
            </tr>
        </thead>
</table> 