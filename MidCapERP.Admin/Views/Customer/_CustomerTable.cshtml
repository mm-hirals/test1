@model IEnumerable<MidCapERP.Dto.Customers.CustomersResponseDto>

<table class="table c-table c-table-stripted w-100" id="tblCustomer">
    <thead>
        <tr>
            <th>
                <input type="checkbox" id="selectall" />
            </th>
            <th>Name</th>
            <th>@Html.DisplayNameFor(model => model.RefferedName)</th>
            <th>@Html.DisplayNameFor(model => model.EmailId)</th>
            <th>@Html.DisplayNameFor(model => model.PhoneNumber)</th>
            <th>@Html.DisplayNameFor(model => model.CreatedDateFormat)</th>
            <th>@Html.DisplayNameFor(model => model.VisitCounts)</th>
            <th class="text-end">Action</th>
        </tr>
    </thead>
</table> 