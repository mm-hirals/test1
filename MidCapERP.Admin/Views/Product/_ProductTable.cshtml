@model IEnumerable<MidCapERP.Dto.Product.ProductResponseDto>

<table class="table c-table c-table-stripted" id="tblProduct">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(model => model.CategoryName)</th>
            <th>@Html.DisplayNameFor(model => model.ProductTitle)</th>
            <th>@Html.DisplayNameFor(model => model.ModelNo)</th>
            <th>@Html.DisplayNameFor(model => model.CreatedDate)</th>
            <th>@Html.DisplayNameFor(model => model.UpdatedDate)</th>
