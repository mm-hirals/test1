@model IEnumerable<MidCapERP.Dto.ContractorCategoryMapping.ContractorCategoryMappingResponseDto>

<table class="table" id="tblContractorCategoryMappingTable">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(model => model.ContractorId)</th>
             <th>@Html.DisplayNameFor(model => model.CategoryId)</th>
            <th>@Html.DisplayNameFor(model => model.IsDeleted)</th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(modelItem => item.ContractorId)</td>
                <td>@Html.DisplayFor(modelItem => item.CategoryId)</td>
                <td>@Html.DisplayFor(modelItem => item.IsDeleted)</td>
                <td>
                    <a asp-controller="ContractorCategoryMapping" asp-action="Update" asp-route-id="@item.ContractorCategoryMappingId"
                   data-ajax-complete="onComplete" data-ajax="true" class="btn btn-primary"
                   data-ajax-mode="replace" data-ajax-update="#UpdateContentDiv">Update</a>
                    <a asp-controller="ContractorCategoryMapping" asp-action="Delete" asp-route-id="@item.ContractorCategoryMappingId"
                   data-ajax-complete="onComplete" data-ajax="true" class="btn btn-primary"
                   data-ajax-mode="replace">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>